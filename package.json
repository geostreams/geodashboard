{
  "description": "Geostreams React library to visualize data from Geostreaming API.",
  "author": "NCSA",
  "license": "NCSA",
  "keywords": [
    "geodashboard",
    "geostreams",
    "NCSA"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geostreams/geodashboard"
  },
  "bugs": {
    "url": "https://github.com/geostreams/geodashboard"
  },
  "homepage": "https://github.com/geostreams/geodashboard",
  "main": "index.js",
  "scripts": {
    "link:all": "for d in packages/*/; do (cd $d && npm link); done",
    "start": "lerna run start --stream --scope",
    "build": "lerna run build --stream --parallel",
    "publish": "lerna publish",
    "test": "lerna run test --stream --scope",
    "postinstall": "lerna bootstrap",
    "lint": "eslint -c .eslintrc --ext .js --ext .jsx --ignore-path .eslintignore ./",
    "lint:fix": "eslint -c .eslintrc --ext .js --ext .jsx --ignore-path .eslintignore --fix ./",
    "flow": "flow",
    "clean": "find . -name 'package-lock.json' -prune -exec rm '{}' + && find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
  },
  "dependencies": {
  }
}
