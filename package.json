{
  "name": "geodashboard",
  "version": "3.3.0",
  "main": "index.js",
  "scripts": {
    "start": "lerna run start --stream --scope",
    "build": "lerna run build --stream --scope",
    "publish": "lerna publish",
    "test": "lerna run test --stream --scope",
    "postinstall": "lerna exec -- npm install && lerna link",
    "lint": "eslint -c .eslintrc --ext .js --ext .jsx --ignore-path .eslintignore ./",
    "lint:fix": "eslint -c .eslintrc --ext .js --ext .jsx --ignore-path .eslintignore --fix ./",
    "flow": "flow",
    "clean": "find . -name 'package-lock.json' -prune -exec rm '{}' + && find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +"
  },
  "author": "NCSA",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "~4.11.0",
    "@material-ui/icons": "~4.9.1",
    "@material-ui/lab": "~4.0.0-alpha.56",
    "@material-ui/styles": "~4.10.0",
    "d3": "~5.16.0",
    "history": "~5.0.0",
    "ol": "~6.4.3",
    "ol-ext": "~3.1.14",
    "ol-layerswitcher": "~3.6.0",
    "react": "~16.13.1",
    "react-dom": "~16.13.1",
    "react-redux": "~7.2.1",
    "react-router-dom": "~5.2.0",
    "redux": "~4.0.5",
    "redux-thunk": "~2.3.0"
  },
  "devDependencies": {
    "@babel/core": "~7.11.4",
    "@babel/plugin-proposal-class-properties": "~7.10.4",
    "@babel/plugin-proposal-export-default-from": "~7.10.4",
    "@babel/plugin-proposal-export-namespace-from": "~7.10.4",
    "@babel/polyfill": "~7.10.4",
    "@babel/preset-env": "~7.11.0",
    "@babel/preset-flow": "~7.10.4",
    "@babel/preset-react": "~7.10.4",
    "@types/d3": "~5.7.2",
    "@types/dom-helpers": "~5.0.1",
    "@types/ol": "~6.3.1",
    "babel-eslint": "~10.1.0",
    "babel-loader": "~8.1.0",
    "clean-webpack-plugin": "~3.0.0",
    "css-loader": "~4.2.1",
    "dom-helpers": "~5.2.0",
    "eslint": "~7.7.0",
    "eslint-config-airbnb": "~18.2.0",
    "eslint-config-prettier": "~6.11.0",
    "eslint-loader": "~4.0.2",
    "eslint-plugin-flowtype": "~5.2.0",
    "eslint-plugin-import": "~2.22.0",
    "eslint-plugin-jsx-a11y": "~6.3.1",
    "eslint-plugin-prettier": "~3.1.4",
    "eslint-plugin-react": "~7.20.6",
    "eslint-plugin-react-hooks": "~4.1.0",
    "favicons-webpack-plugin": "~4.2.0",
    "file-loader": "~6.0.0",
    "flow-bin": "~0.131.0",
    "html-webpack-plugin": "~4.3.0",
    "husky": "~4.2.5",
    "lerna": "~3.22.1",
    "less": "~3.12.2",
    "less-loader": "~6.2.0",
    "lint-staged": "~10.2.11",
    "mini-css-extract-plugin": "~0.10.0",
    "optimize-css-assets-webpack-plugin": "~5.0.3",
    "prettier": "~2.0.5",
    "svg-inline-loader": "~0.8.2",
    "webpack": "~4.44.1",
    "webpack-bundle-analyzer": "~3.8.0",
    "webpack-cli": "~3.3.12",
    "webpack-dev-server": "~3.11.0",
    "webpack-merge": "~5.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "flow && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint -c .eslintrc --ignore-path .eslintignore"
    ]
  }
}
