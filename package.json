{
  "name": "geodashboard",
  "version": "3.3.0",
  "main": "index.js",
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "scripts": {
    "start": "lerna run start --stream --scope",
    "build": "lerna run build --stream --scope",
    "publish": "lerna publish",
    "test": "lerna run test --stream --scope",
    "postinstall": "lerna exec -- npm install && lerna link",
    "lint": "eslint -c .eslintrc --ext .js --ext .jsx --ignore-path .eslintignore ./",
    "lint:fix": "eslint -c .eslintrc --ext .js --ext .jsx --ignore-path .eslintignore --fix ./",
    "flow": "flow",
    "clean": "find . -name 'package-lock.json' -prune -exec rm '{}' + && find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +"
  },
  "author": "NCSA",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "~4.9.7",
    "@material-ui/icons": "~4.9.1",
    "@material-ui/styles": "~4.9.6",
    "connected-react-router": "~6.8.0",
    "d3": "~5.15.0",
    "history": "~4.10.1",
    "immutable": "~4.0.0-rc.12",
    "ol": "~6.2.1",
    "ol-ext": "~3.1.10",
    "ol-layerswitcher": "~3.5.0",
    "react": "~16.13.1",
    "react-dom": "~16.13.1",
    "react-redux": "~7.2.0",
    "react-router-dom": "~5.1.2",
    "redux": "~4.0.5",
    "redux-immutable": "~4.0.0",
    "redux-thunk": "~2.3.0"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@babel/plugin-proposal-class-properties": "~7.8.3",
    "@babel/plugin-proposal-export-default-from": "~7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "~7.8.3",
    "@babel/polyfill": "~7.8.7",
    "@babel/preset-env": "~7.9.0",
    "@babel/preset-flow": "~7.9.0",
    "@babel/preset-react": "~7.9.4",
    "@types/d3": "~5.7.2",
    "@types/dom-helpers": "~5.0.1",
    "babel-eslint": "~10.1.0",
    "babel-loader": "~8.1.0",
    "clean-webpack-plugin": "~3.0.0",
    "css-loader": "~3.4.2",
    "dom-helpers": "~5.1.3",
    "eslint": "~6.8.0",
    "eslint-config-airbnb": "~18.1.0",
    "eslint-config-prettier": "~6.10.1",
    "eslint-loader": "~3.0.3",
    "eslint-plugin-flowtype": "~4.7.0",
    "eslint-plugin-import": "~2.20.1",
    "eslint-plugin-jsx-a11y": "~6.2.3",
    "eslint-plugin-prettier": "~3.1.2",
    "eslint-plugin-react": "~7.19.0",
    "eslint-plugin-react-hooks": "~2.5.1",
    "favicons-webpack-plugin": "~3.0.1",
    "file-loader": "~6.0.0",
    "flow-bin": "~0.121.0",
    "html-webpack-plugin": "~4.0.1",
    "husky": "~4.2.3",
    "lerna": "~3.20.2",
    "less": "~3.11.1",
    "less-loader": "~5.0.0",
    "lint-staged": "~10.0.9",
    "mini-css-extract-plugin": "~0.9.0",
    "optimize-css-assets-webpack-plugin": "~5.0.3",
    "prettier": "~2.0.2",
    "svg-inline-loader": "~0.8.2",
    "webpack": "~4.42.1",
    "webpack-bundle-analyzer": "~3.6.1",
    "webpack-cli": "~3.3.11",
    "webpack-dev-server": "~3.10.3",
    "webpack-merge": "~4.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "flow && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint -c .eslintrc --ignore-path .eslintignore"
    ]
  }
}
